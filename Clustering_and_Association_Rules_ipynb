    {
      "cell_type": "code",
      "source": [
        "def Cluster(clr):\n",
        "    if clr == 0:\n",
        "        return 'новички'\n",
        "    elif clr == 1:\n",
        "        return 'старички'\n",
        "    elif clr == 2:\n",
        "        return 'приведения'\n",
        "    elif clr == 3:\n",
        "        return 'среднячки'\n",
        "\n",
        "# Добавим новый столбец \"Description_Cluster\"\n",
        "data_df['Description_Cluster'] = data_df['Description'] + \"_\" + data_df['Cluster'].apply(Cluster)"
      ],
      "metadata": {
        "id": "SHUQKfE0aXQ3"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from mlxtend.frequent_patterns import apriori\n",
        "from mlxtend.frequent_patterns import association_rules\n",
        "!pip install apyori\n",
        "from apyori import apriori"
      ],
      "metadata": {
        "id": "A_C0X3QsNAQM"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "transactions_3 = data_df.groupby('Invoice')['Description_Cluster'].apply(list).values.tolist()\n",
        "\n",
        "association_rules = apriori(transactions_3, min_support=0.002, min_confidence=0.5, min_lift=1, min_length=2)\n",
        "association_results = list(association_rules)"
      ],
      "metadata": {
        "id": "bd6eAMg39Cjf"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Создадим три группы для рекомендаций\n",
        "useful_rules = []\n",
        "trivial_rules = []\n",
        "non_obvious_rules = []"
      ],
      "metadata": {
        "id": "UePz8-TRTXSl"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Разделение правил на группы\n",
        "for rule in association_results:\n",
        "    items = [item for item in rule.items]\n",
        "    support = rule.support\n",
        "    confidence = rule.ordered_statistics[0].confidence\n",
        "    lift = rule.ordered_statistics[0].lift\n",
        "\n",
        "    # Определяем тип правила\n",
        "    if confidence >= 0.6 and lift >= 150:\n",
        "        trivial_rules.append((items, support, confidence, lift))\n",
        "    elif confidence >= 0.55 and lift >= 120:\n",
        "         useful_rules.append((items, support, confidence, lift))\n",
        "    else:\n",
        "        non_obvious_rules.append((items, support, confidence, lift))"
      ],
      "metadata": {
        "id": "CqooOEt7TeO1"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "print(\"Полезные правила:\")\n",
        "for rule in useful_rules:\n",
        "    base_str = ', '.join(rule[0][:-1])\n",
        "    add_str = rule[0][-1]\n",
        "    print('Если куплено ({}), то будет куплено ({}) sup = {}, conf = {}, lift = {}'.format(base_str, add_str, str(round(rule[1], 3)), str(round(rule[2], 3)), str(round(rule[3], 3))))\n",
        "\n",
        "print()\n",
        "print(\"\\nТривиальные правила:\")\n",
        "for rule in trivial_rules:\n",
        "    base_str = ', '.join(rule[0][:-1])\n",
        "    add_str = rule[0][-1]\n",
        "    print('Если куплено ({}), то будет куплено ({}) sup = {}, conf = {}, lift = {}'.format(base_str, add_str, str(round(rule[1], 3)), str(round(rule[2], 3)), str(round(rule[3], 3))))\n",
        "\n",
        "print()\n",
        "print(\"\\nНеочевидные правила:\")\n",
        "for rule in non_obvious_rules:\n",
        "    base_str = ', '.join(rule[0][:-1])\n",
        "    add_str = rule[0][-1]\n",
        "    print('Если куплено ({}), то будет куплено ({}) sup = {}, conf = {}, lift = {}'.format(base_str, add_str, str(round(rule[1], 3)), str(round(rule[2], 3)), str(round(rule[3], 3))))"
